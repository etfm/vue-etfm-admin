{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@etfm/vea-ts/web.json",
  "compilerOptions": {
    "baseUrl": ".",
    "types": [
      "element-plus/global",
      "../../typings/global",
      "../../typings/env",
      "unplugin-vue-define-options/macros-global"
    ],
    "paths": {
      "@/*": ["src/*"],
      "#/*": ["types/*"]
    }
  },
  "vueCompilerOptions": {
    "target": 3,
    "plugins": ["@vue-macros/volar/define-options"]
  },
  "include": ["mock", "src", ".etfmirc.ts", ".etfm.ts", "vite.config.ts", "vitest.config.ts"]
}
