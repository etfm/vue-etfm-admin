{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@etfm/vea-ts/web.json",
  "compilerOptions": {
    "baseUrl": ".",
    "types": ["unplugin-vue-define-options/macros-global", "vite/client"],
    "moduleResolution": "bundler"
  },
  "vueCompilerOptions": {
    "target": 3,
    "plugins": ["@vue-macros/volar/define-options"]
  },
  "include": ["src"]
}
