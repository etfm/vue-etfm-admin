{
  "name": "root",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "bootstrap": "pnpm install",
    "clean": "rimraf node_modules",
    "reinstall": "pnpm clean && pnpm bootstrap",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/",
    "prepare": "is-ci || husky install",
    "lint:lint-staged": "lint-staged -c ./.lintstagedrc",
    "commit": "czg",
    "build": "turbo build --filter=./apps/element-admin",
    "dev": "turbo dev --filter=./apps/element-admin --no-daemon --include-dependencies"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@etfma/design": "workspace:^0.0.0",
    "@iconify/iconify": "^3.1.0",
    "element-plus": "^2.3.2",
    "vue": "^3.3.4",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.1",
    "@etfma/eslint": "workspace:*",
    "@etfma/prettier": "workspace:*",
    "@etfma/stylelint": "workspace:^0.0.0",
    "@etfma/ts": "workspace:*",
    "@etfma/vite": "workspace:*",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.14.2",
    "@vue-macros/volar": "^0.9.5",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.1.3",
    "cz-git": "^1.6.1",
    "czg": "^1.6.1",
    "fast-glob": "^3.2.12",
    "husky": "^8.0.0",
    "is-ci": "^3.0.1",
    "jsdom": "^21.1.0",
    "less": "^4.1.3",
    "lint-staged": "^13.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^4.4.1",
    "sass": "^1.62.0",
    "start-server-and-test": "^2.0.0",
    "turbo": "^1.9.3",
    "typescript": "~5.0.4",
    "unbuild": "^1.2.0",
    "unplugin-vue-define-options": "^1.3.3",
    "vite": "4.3.0-beta.6",
    "vitest": "^0.29.1",
    "vue-tsc": "^1.8.3"
  }
}
